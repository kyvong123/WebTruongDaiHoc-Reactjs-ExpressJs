{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "globals": {
        "T": "writable",
        "$": "writable",
        "CKEDITOR": "writable",
        "Chart": "writable"
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "parser": "babel-eslint",
    "ignorePatterns": [
        "/modules/mdHanhChinhTongHop/hcthKyDienTu/pkijs.es.js",
        "view/js/*",
        "public/*",
        "asset/*",
        "asset/**"
    ],
    "rules": {
        "no-constant-condition": "off",
        "react/no-unknown-property": "off",
        "no-unused-vars": [
            "error"
        ],
        "indent": [
            "off"
        ],
        "no-var": [
            "error"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "react/prop-types": "off",
        "react/display-name": [
            0
        ],
        "no-useless-escape": "off",
        "no-prototype-builtins": "off"
    }
}